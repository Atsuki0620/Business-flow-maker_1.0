    <bpmn2:userTask id="t1_rfc_submit" name="変更リクエスト登録" />
    <bpmn2:userTask id="t2_region_review" name="地域代表レビュー" />
    <bpmn2:userTask id="t3_global_sync" name="CAB レビュー準備" />
    <bpmn2:userTask id="t4_execute" name="変更実行と結果記録" />
    <bpmn2:exclusiveGateway id="g1_risk_level" name="リスクレベル判定" />
