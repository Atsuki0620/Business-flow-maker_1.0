    <bpmn2:sequenceFlow id="f1" sourceRef="t1_rfc_submit" targetRef="t2_region_review" />
    <bpmn2:sequenceFlow id="f2" sourceRef="t2_region_review" targetRef="g1_risk_level" />
    <bpmn2:sequenceFlow id="f3" sourceRef="g1_risk_level" targetRef="t3_global_sync">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">高リスク</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:sequenceFlow id="f4" sourceRef="g1_risk_level" targetRef="g2_emergency">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">低〜中リスク</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:sequenceFlow id="f5" sourceRef="g2_emergency" targetRef="t4_execute">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">緊急</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:sequenceFlow id="f6" sourceRef="t3_global_sync" targetRef="t4_execute" />
    <bpmn2:sequenceFlow id="f7" sourceRef="g2_emergency" targetRef="t3_global_sync">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">通常</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
