      <bpmn2:lane id="Lane_regional_apac" name="APAC運用チーム">
        <bpmn2:flowNodeRef>t3_global_sync</bpmn2:flowNodeRef>
      </bpmn2:lane>
      <bpmn2:lane id="Lane_cab_board" name="CAB">
        <bpmn2:flowNodeRef>t4_execute</bpmn2:flowNodeRef>
